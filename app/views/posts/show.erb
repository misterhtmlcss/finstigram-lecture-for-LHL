<header>
  <h1>Finstagram</h1>
  <div class="login-info">
    <% if current_user %>
      <a href="/logout"><%= current_user.username %></a>
    <% else %>
      <a href="/login">Login</a>
      <a href="/signup">Signup</a>
    <% end %>
  </div>
</header>
<main role="main">
  <%= erb(:'shared/post', { locals: { finstagram_post: @finstagram_post } } ) %>
</main>
<script src="js/lib.js"></script>